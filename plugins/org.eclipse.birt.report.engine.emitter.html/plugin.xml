<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension 
	 	point="org.eclipse.birt.report.engine.emitters"
	 	name = "Engine html Emitters">
	 	<emitter
         class="org.eclipse.birt.report.engine.emitter.html.HTMLReportEmitter"
         fileExtension="html"
         format="html"
         icon="resource/preview_html.gif"
         id="org.eclipse.birt.report.engine.emitter.html"
         mimeType="text/html"
         outputDisplayNone="true"
         pagination="page-break-pagination">
	 	</emitter>
   </extension>
</plugin>
